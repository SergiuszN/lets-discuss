{% extends '@App/layout.html.twig' %}

{% block body %}
    {% include '@App/companyAdmin/components/navbar.html.twig' %}

    {% for type, flash_messages in app.session.flashBag.all %}
        {% for flash_message in flash_messages %}

            <div class="alert alert-{{ type }} alert-dismissible fade in">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>{{ flash_message }}</strong>
            </div>

        {% endfor %}
    {% endfor %}

    <ol class="breadcrumb">
        <li><a href="{{ path('app_company_admin_homepage') }}">Dashboard</a></li>
        <li><a href="{{ path('app_company_admin_manager_list') }}">Manager List</a></li>
        <li class="active">Manager Add</li>
    </ol>

    <div class="page-header">
        <h1>Manager Add <br class="visible-xs"><small>Create new manager</small></h1>
    </div>

    {{ form(form) }}
{% endblock %}